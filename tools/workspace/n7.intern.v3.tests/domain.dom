domain Math{
    imports {
        @python {
            NP : numpy
        }
    }
    def Operation( float, float, float, float )  :  float  {
        MATRICE_MULTIPLATION  {
            @python (NP::matmul)
        }
        
        MATRICE_SUM  {
            @python  (NP::sum)
        }
    }
    

    Operation operate = Operation.MATRICE_SUM {
        @do(Interpolation) {
            this($1,$2)
        }
    }
}