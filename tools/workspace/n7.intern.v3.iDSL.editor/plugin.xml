-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated iDSL -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="iDSL.presentation.IDSLModelWizardID"
            name="%_UI_IDSLModelWizard_label"
            class="iDSL.presentation.IDSLModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/IDSLModelFile.gif">
         <description>%_UI_IDSLModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated iDSL -->
      <editor
            id="iDSL.presentation.IDSLEditorID"
            name="%_UI_IDSLEditor_label"
            icon="icons/full/obj16/IDSLModelFile.gif"
            extensions="idsl"
            class="iDSL.presentation.IDSLEditor"
            contributorClass="iDSL.presentation.IDSLActionBarContributor">
      </editor>
   </extension>

</plugin>
